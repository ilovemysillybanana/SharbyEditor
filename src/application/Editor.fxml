<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="gridPane" gridLinesVisible="true" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="611.0" prefWidth="829.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.EditorController">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
			prefWidth="100.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints percentHeight="5.0" valignment="CENTER"
			vgrow="NEVER" />
		<RowConstraints maxHeight="1.7976931348623157E308"
			percentHeight="90.0" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="-Infinity" percentHeight="5.0"
			valignment="BOTTOM" vgrow="NEVER" />
	</rowConstraints>
	<children>
		<MenuBar prefHeight="5.0" prefWidth="829.0">
			<menus>
				<Menu mnemonicParsing="false" text="File">
					<items>
						<MenuItem fx:id="menu_btn_Open" mnemonicParsing="false"
							onAction="#btnOpen" text="Open..." />
						<MenuItem fx:id="menu_btn_New" mnemonicParsing="false"
							text="New..." />
						<MenuItem fx:id="menu_btn_Save" mnemonicParsing="false"
							text="Save" />
						<MenuItem fx:id="menu_btn_Exit" mnemonicParsing="false"
							text="Exit" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Edit">
					<items>
						<MenuItem fx:id="menu_btn_Copy" mnemonicParsing="false"
							text="Copy" />
						<MenuItem fx:id="menu_btn_Cut" mnemonicParsing="false"
							text="Cut" />
						<MenuItem fx:id="menu_btn_Delete" mnemonicParsing="false"
							text="Delete" />
						<MenuItem fx:id="menu_btn_Select" mnemonicParsing="false"
							text="Select" />
						<MenuItem fx:id="menu_btn_SelectAll" mnemonicParsing="false"
							text="Select All" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Settngs">
					<items>
						<MenuItem fx:id="menu_btn_Run" mnemonicParsing="false"
							text="Run" />
						<MenuItem fx:id="menu_btn_RunSettings" mnemonicParsing="false"
							text="Run Settings..." />
						<MenuItem mnemonicParsing="false" text="Working Directory..." />
					</items>
				</Menu>
				<Menu fx:id="menu_btn_About" mnemonicParsing="false" onAction="#btnAbout"
					text="Help">
					<items>
						<MenuItem mnemonicParsing="false" text="About" />
					</items>
				</Menu>
			</menus>
		</MenuBar>
		<GridPane gridLinesVisible="true" GridPane.rowIndex="1">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" percentWidth="20.0" />
				<ColumnConstraints hgrow="SOMETIMES" percentWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<TreeView fx:id="filesTree" editable="true"
					GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
					GridPane.valignment="BOTTOM" GridPane.vgrow="ALWAYS" />
				<TabPane fx:id="tabPaneCode" rotateGraphic="true"
					tabClosingPolicy="ALL_TABS" GridPane.columnIndex="1"
					GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
					GridPane.valignment="TOP" GridPane.vgrow="ALWAYS">
					<tabs>
					</tabs>
				</TabPane>
			</children>
		</GridPane>
	</children>
</GridPane>
